    <style name="ConversationItemStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/choice_input_type_width</item>
        <item name="android:layout_marginRight">@dimen/choice_input_type_width</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="FullScreenDialogTheme">
        <item name="android:windowNoTitle">true</item>    
    	<item name="android:windowFullscreen">true</item>  
    	<item name="android:windowSwipeToDismiss">false</item>
    </style>
    <style name="InputTypeStyle">
        <item name="android:layout_width">@dimen/input_type_btn_width</item>
        <item name="android:layout_height">@dimen/input_type_btn_width</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_marginRight">45px</item>
        <item name="android:layout_marginBottom">68px</item>
        <item name="android:visibility">invisible</item>
        <item name="android:clickable">true</item>
        <item name="android:padding">10px</item>
    </style>
    <style name="SwipeBackLayout">
        <item name="edge_size">50dip</item>
        <item name="shadow_left">@drawable/shadow_left</item>
        <item name="shadow_right">@drawable/shadow_right</item>
        <item name="shadow_bottom">@drawable/shadow_bottom</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="SwipeToDismissTheme" parent="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen">
        <item name="android:windowSwipeToDismiss">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
	    <item name="android:windowIsTranslucent">true</item>
	    <item name="android:keycode">KEYCODE_SOFT_LEFT</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
	    <!-- <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item> -->
    </style>
    <style name="UserNameStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15px</item>
        <item name="android:textColor">@color/username_color</item>
    </style>
